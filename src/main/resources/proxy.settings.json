{
  "vertx": {
    "metricsOptions": {
      "enabled": false,
      "jvmMetricsEnabled": true,
      "labels": [
        "HTTP_METHOD",
        "HTTP_CODE",
        "HTTP_PATH"
      ],
      "prometheusOptions": {
        "enabled": false,
        "startEmbeddedServer": true,
        "embeddedServerOptions": {
          "port": 8081
        }
      },
      "oltpOptions": {
        "enabled": false
      }
    }
  },
  "client": {
    "idleTimeoutUnit": "MILLISECONDS",
    "idleTimeout": 300000,
    "connectTimeout": 10000,
    "keepAlive": true,
    "maxPoolSize": 128
  },
  "server": {
    "port": 8080,
    "acceptBacklog": 4096,
    "idleTimeoutUnit": "MILLISECONDS",
    "idleTimeout": 60000,
    "compressionSupported": true
  },
  "identityProvider": {
    "jwksUrl": "https://kctest.staging.deltixhub.io/realms/EPAM/protocol/openid-connect/certs",
    "appName": "dial"
  }
}