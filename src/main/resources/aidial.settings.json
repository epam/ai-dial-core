{
  "vertx": {
    "workerPoolSize": 32,
    "metricsOptions": {
      "enabled": true,
      "disabledMetricsCategories": ["http.server"],
      "jvmMetricsEnabled": true,
      "labels": [
        "HTTP_METHOD",
        "HTTP_CODE",
        "HTTP_PATH"
      ],
      "prometheusOptions": {
        "enabled": true,
        "startEmbeddedServer": true,
        "embeddedServerOptions": {
          "port": 9464
        }
      },
      "oltpOptions": {
        "enabled": false,
        "otlp.url": ""
      }
    }
  },
  "client": {
    "idleTimeoutUnit": "MILLISECONDS",
    "idleTimeout": 300000,
    "connectTimeout": 10000,
    "keepAlive": true,
    "maxPoolSize": 128
  },
  "server": {
    "port": 8080,
    "acceptBacklog": 4096,
    "idleTimeoutUnit": "MILLISECONDS",
    "idleTimeout": 300000,
    "compressionSupported": true
  },
  "config": {
    "files": [],
    "reload": 60000
  },
  "identityProviders": {
  },
  "storage": {
    "provider" : "filesystem",
    "bucket": "dial",
    "createBucket": true,
    "overrides": {
      "jclouds.filesystem.basedir": "data"
    }
  },
  "resources": {
    "maxSize" : 1048576,
    "syncPeriod": 60000,
    "syncDelay": 120000,
    "syncBatch": 4096,
    "cacheExpiration": 300000,
    "compressionMinSize": 256
  },
  "applications": {
    "includeCustomApps": false
  },
  "access": {
    "admin": {
      "rules": [
        {
          "function": "CONTAIN",
          "source": "roles",
          "targets": ["admin"]
        }
      ]
    }
  }
}
