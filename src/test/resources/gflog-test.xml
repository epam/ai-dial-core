<config>
    <appender name="console" factory="com.epam.deltix.gflog.core.appender.ConsoleAppenderFactory"/>

    <logger level="INFO">
        <appender-ref ref="console"/>
    </logger>

    <logger name="io.vertx" level="TRACE">
        <appender-ref ref="console"/>
    </logger>

    <logger name="io.netty.handler.logging.LoggingHandler" level="TRACE">
        <appender-ref ref="console"/>
    </logger>
    <service/>
</config>